{
    "name": "infinity54/riotapiphpwrapper",
    "description": "Package to use Riot Games APIs more easily and more quickly in your PHP projects.",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "php-curl-class/php-curl-class": "^9.14"
    },
    "autoload": {
        "psr-4": {
            "Riot\\": "src",
            "Riot\\API\\": "src/API",
            "Riot\\Constants\\": "src/Constants",
            "Riot\\DTO\\": "src/DTO",
            "Riot\\Exception\\": "src/Exception"
        }
    }
}
